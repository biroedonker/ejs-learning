<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>EJS Templating</title>
  </head>
  <body>
    <h1><%= title %></h1>
    <p>
      Current second:
      <!-- Current second goes here -->
      <%= seconds %>
    </p>
    <% if(seconds%2==0){%>
    <ul>
      <% items.forEach(function(item){ %>
      <li><%= item %></li>
      <% }) %>
    </ul>
    <%} else{%>
    <p>No item to display</p>
    <% } %>
    <p><%-htmlContent %></p>
    <%-include("footer.ejs") %>
  </body>
</html>
